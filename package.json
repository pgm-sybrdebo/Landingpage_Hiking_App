{
  "name": "exercises-pgm-sybrdebo",
  "version": "1.0.0",
  "description": "An simple landing page in Eleventy",
  "main": "index.js",
  "dependencies": {
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "eleventy": "npx @11ty/eleventy",
    "sass": "sass --style=compressed src/style/main.scss src/main.css",
    "watch:sass": "npm run sass -- --watch",
    "watch:eleventy": "eleventy --serve",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npm run eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgmgent-2021-uip/exercises-pgm-sybrdebo.git"
  },
